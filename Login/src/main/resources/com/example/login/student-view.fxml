                <TableColumn fx:id="courseName" prefWidth="310.0" text="Course Name" />
                <TableColumn fx:id="CRN" minWidth="0.0" prefWidth="289.0" text="CRN" />
            </columns>
        </TableView>
    </children>
</AnchorPane>
