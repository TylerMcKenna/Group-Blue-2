<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.login.ProfessorController">
    <children>
        <TableView fx:id="courseTable" layoutY="25.0" onMouseClicked="#coursePressed" prefHeight="200.0" prefWidth="294.0">
            <columns>
                <TableColumn fx:id="courseName" prefWidth="150.0" text="Course Name" />
                <TableColumn fx:id="CRN" minWidth="0.0" prefWidth="142.0" text="CRN" />
            </columns>
        </TableView>
        <TableView fx:id="studentTable" layoutX="296.0" layoutY="25.0" prefHeight="200.0" prefWidth="304.0">
            <columns>
                <TableColumn fx:id="userName" prefWidth="147.0" text="Student Name" />
                <TableColumn fx:id="email" prefWidth="156.0" text="Student Email" />
            </columns>
        </TableView>
      <Button fx:id="addClass" layoutX="14.0" layoutY="235.0" mnemonicParsing="false" text="Add Class" />
      <Button fx:id="deleteClass" layoutX="14.0" layoutY="270.0" mnemonicParsing="false" text="Delete Class" />
      <TextField fx:id="CRNTextField" layoutX="145.0" layoutY="235.0" prefHeight="25.0" prefWidth="133.0" />
      <Label layoutX="110.0" layoutY="239.0" text="CRN:" />
      <Button fx:id="addStudent" layoutX="300.0" layoutY="235.0" mnemonicParsing="false" text="Add Student" />
      <Button fx:id="deleteStudent" layoutX="300.0" layoutY="270.0" mnemonicParsing="false" text="Delete Student" />
      <Label layoutX="411.0" layoutY="239.0" text="B#:" />
      <TextField fx:id="BNUMTextField" layoutX="436.0" layoutY="235.0" prefHeight="25.0" prefWidth="142.0" />
      <Button fx:id="UpdateButton" layoutX="14.0" layoutY="304.0" onMouseClicked="#updatePressed" mnemonicParsing="false" text="Update Class" />
      <TextField fx:id="CourseNameReplace" layoutX="110.0" layoutY="304.0" prefHeight="25.0" prefWidth="168.0" />
      <TextField fx:id="CourseNumReplace" layoutX="110.0" layoutY="339.0" prefHeight="25.0" prefWidth="168.0" />
    </children>
</AnchorPane>
